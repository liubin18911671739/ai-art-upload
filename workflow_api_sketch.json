{
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    },
    "_meta": {
      "title": "Load Checkpoint"
    }
  },
  "2": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "https://example.com/input.jpg",
      "upload": "image"
    },
    "_meta": {
      "title": "Injected Source Image (URL)"
    }
  },
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "masterpiece, best quality, realistic pencil sketch, graphite texture, monochrome, clean contour lines, cross-hatching",
      "clip": [
        "1",
        1
      ]
    },
    "_meta": {
      "title": "Positive Prompt (Sketch)"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "low quality, watercolor, oil paint, over-saturated, blurry, bad anatomy, watermark, text",
      "clip": [
        "1",
        1
      ]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Init Latent"
    }
  },
  "6": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 123456789,
      "steps": 30,
      "cfg": 6.8,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1,
      "model": [
        "1",
        0
      ],
      "positive": [
        "3",
        0
      ],
      "negative": [
        "4",
        0
      ],
      "latent_image": [
        "5",
        0
      ]
    },
    "_meta": {
      "title": "Sampler"
    }
  },
  "7": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": [
        "6",
        0
      ],
      "vae": [
        "1",
        2
      ]
    },
    "_meta": {
      "title": "Decode Output"
    }
  },
  "8": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "runpod-style-sketch",
      "images": [
        "7",
        0
      ]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
